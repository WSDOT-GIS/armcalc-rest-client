{
  "name": "armcalc-rest-client",
  "version": "1.0.4",
  "description": "Client for WSDOT ARMCalc REST API",
  "main": "armcalc-client.js",
  "scripts": {
    "build:babel": "babel es2015 -d .",
    "prepublish": "typings install --production && tsc --project tsconfigNoTest.json && npm run build:babel",
    "build": "typings install && tsc && npm run build:babel",
    "pretest": "npm run-script build",
    "test:live": "jasmine spec/armcalc-clientSpec.js",
    "test": "jasmine spec/armcalc-clientSpec-fake.js && karma start --browsers Firefox"
  },
  "private": true,
  "keywords": [
    "wsdot",
    "armcalc",
    "srmp",
    "arm",
    "milepost"
  ],
  "author": "WSDOT",
  "contributors": [
    {
      "name": "Jeff Jacobson",
      "email": "JeffJacobson@users.noreply.github.com",
      "url": "https://github.com/JeffJacobson"
    }
  ],
  "engines": {
    "node": ">=6.3.1"
  },
  "license": "Unlicense",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "core-js": "^2.4.1",
    "jasmine": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "requirejs": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/armcalc-rest-client.git"
  },
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/armcalc-rest-client/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/armcalc-rest-client#readme"
}
